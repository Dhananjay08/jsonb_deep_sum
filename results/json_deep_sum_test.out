create extension json_deep_sum;
select json_list('{"a": 2, "b": 1}', '{"a": 5}');
    json_list     
------------------
 {"a": 7, "b": 1}
(1 row)

select json_list('{"a": 2, "b": 3}', '{"a": 5, "b": 4}');
    json_list     
------------------
 {"a": 7, "b": 7}
(1 row)

select json_list('{"b": 1}', '{"a": 2}');
    json_list     
------------------
 {"a": 2, "b": 1}
(1 row)

select json_list('{}', '{"a": 2}');
 json_list 
-----------
 {"a": 2}
(1 row)

select json_list('1', '{"a": 1}');
ERROR:  Can only sum objects
